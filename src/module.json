{
  "id": "junologue-chorus",
  "name": "Junologue Chorus",
  "abbrev": "JC",
  "version": "0.1.0",
  "description": "Juno-60 chorus emulation - port of peterall/junologue-chorus",
  "author": "Peter Allwin (port: charlesvestal)",
  "license": "MIT",
  "dsp": "junologue-chorus.so",
  "api_version": 2,
  "capabilities": {
    "chainable": true,
    "component_type": "audio_fx",
    "ui_hierarchy": {
      "levels": {
        "root": {
          "name": "Junologue Chorus",
          "params": [
            {
              "key": "mode",
              "label": "Mode",
              "type": "enum",
              "options": [
                "I",
                "I+II",
                "II"
              ],
              "default": "I+II"
            },
            {
              "key": "mix",
              "label": "Mix",
              "type": "float",
              "min": 0.0,
              "max": 1.0,
              "default": 0.5,
              "step": 0.01,
              "unit": "%"
            },
            {
              "key": "brightness",
              "label": "Brightness",
              "type": "float",
              "min": 0.0,
              "max": 1.0,
              "default": 1.0,
              "step": 0.01,
              "unit": "%"
            }
          ],
          "knobs": [
            "mode",
            "mix",
            "brightness"
          ]
        }
      }
    }
  }
}
